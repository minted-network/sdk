{
  "name": "@minted-network/sdk",
  "version": "1.1.1",
  "license": "MIT",
  "engines": {
    "node": ">= 16.14.0",
    "pnpm": ">= 8.6.0"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/src/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Minted Network",
  "keywords": [
    "minted"
  ],
  "homepage": "https://github.com/minted-network/sdk#readme",
  "bugs": "https://github.com/minted-network/sdk/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minted-network/sdk.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix"
  },
  "scripts": {
    "prepare": "husky install",
    "prebuild": "rm -rf ./dist",
    "build": "rollup -c",
    "fix": "eslint src --fix",
    "lint": "eslint --max-warnings 0 'src'",
    "ts-check": "tsc --noEmit",
    "typechain": "typechain --out-dir src/types/contracts --target ethers-v5 'src/abis/*.json'",
    "format:check": "prettier --check 'src/**/*.{js,jsx,ts,tsx,json,yaml,yml,md}'",
    "format:write": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,yaml,yml,md}'",
    "test": "jest",
    "publish-pkg": "pnpm build && changeset version && changeset publish"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.6.3",
    "@ethersproject/providers": "^5.6.8"
  },
  "peerDependencies": {
    "ethers": "^5.6.8"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-typescript": "^11.1.0",
    "@typechain/ethers-v5": "^10.1.0",
    "@types/jest": "^29.5.1",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "bufferutil": "^4.0.7",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsonc": "^2.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "ethers": "^5.6.8",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "rollup": "^3.21.6",
    "rollup-plugin-copy": "^3.4.0",
    "ts-jest": "^29.1.0",
    "typechain": "^8.1.0",
    "typescript": "^5.0.4",
    "utf-8-validate": "^6.0.3"
  }
}
